  </head>

  <body>
   

<br> 


<h1>MillieFetch Upload</h1>
<% if !flash[:notice].blank? %>
   <div class = "alert alert-info">
      <%= flash[:notice] %>
   </div>
<% end %>  
  
<br />   

<%#= button_to "New Upload", '/uploads/new', class: "btn btn-primary" #%>
<table>
<tr><td><%= button_to "Upload documents", '/uploads/series', class: "btn btn-primary", method: :get%></td>
<% if @mul_uploads != 0 %>
<td><%= button_to "Documents awaiting information (#{@mul_uploads})", '/multiform/index', class: "btn btn-primary", method: :get%></td>
<% end %>
</tr>
</table>
<%#= button_to "Manage documents", document_manager_path, class: "btn btn-primary", method: :get%>
<%#= link_to "New Upload", new_upload_path, class: "btn btn-primary"#%> 
<br />   
<br />
   <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
